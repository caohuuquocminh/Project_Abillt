                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>

                  </button>

                  
                </div>

                <div id="navbar" class="navbar-collapse collapse pull-left" aria-label="Main menu" role="navigation">

                  <ul class="nav navbar-nav" role="menubar" id="
                  nav">

                    <li><%= link_to "HOME", root_path %></li>
                    <li><%= link_to "CONTACT", new_message_path %></li>
                    <li class="has-child" role="menuitem" tabindex="1" aria-haspopup="true">
                      <a href="#">CATEGORIES</a>
                      <ul class="submenu" role="menu" aria-hidden="false">
                        <% Category.all.each do |category| %>
                          <li role="menuitem" tabindex="0" class="<%= 'active' if params[:category] == category.name %>">
                            <%= link_to category.name, apps_path(category: category.name), class: "link",  :tabindex=>"0"%>
                          </li>
                        <% end %>
                      </ul>
                      <p>
                    </li>
                    <li class="has-child">
                      <a href="#">LANGUAGES</a>
                      <ul class="submenu">
                        <li class="submenu-item"><%= link_to_unless I18n.locale == :en, "English", set_language_english_path %></li>
                        <li class="submenu-item"><%= link_to_unless I18n.locale == :de, "Deutsch", set_language_german_path %></li>
                      </ul>
                    </li>
                    <!--  -->
                  </ul>
                </div> 
                <div id="navbar" class="navbar-collapse collapse pull-right">

                  <%= form_tag apps_path, method: :get, class: "navbar-form navbar-right", role: "search" do %>
                      <p>
                       <%= text_field_tag :search, params[:search], class: "form-control", placeholder:"Searchfield" %>
                       <%= submit_tag "SEARCH", name: nil, class: "btn btn-default" %>
                       </p>
                      <% end %>
                </div><!-- /#navbar -->

              </div> <!-- /.container -->
              
            </div> <!-- /.navbar-main -->


        </nav> 

